{
  "initilizationOptions": {
    "completionDisableFilterText": true
  },
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "NodeNext", // or "Bundler"
    "baseUrl": "./", // Base directory for resolving non-relative module names
    "include": ["./src"],
    "paths": {
      "@/*": ["src/*"]
    }
  }
}

